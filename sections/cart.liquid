<div class="padding-global padding-section-small">
          <div class="container-large">
            <div class="headline-wrapper">
              <h2 id="w-node-_4ff72092-e0e6-d591-6af1-5b10f594a629-f594a625" class="heading-style-h2">{{ section.settings.text_heading }}</h2>
              <a href="{{ routes.root_url }}" li-object:href="routes.root_url" class="text-size-small text-style-link hide-mobile-landscape"><span>{{ section.settings.text_continue_shopping }}</span></a>
            </div>
            {% if cart.item_count &gt; 0 %}<div li-if="cart.item_count &gt; 0" class="cart_form-block w-form">
              <form method="post" li-form:cart="" name="email-form" data-name="Email Form" li-form="cart" id="email-form" class="cart_grid-wrapper" data-wf-page-id="68e5827dc33ba2f819dc558f" data-wf-element-id="4ff72092-e0e6-d591-6af1-5b10f594a62f">
                <div class="cart_table outline-solid-1">
                  <div class="cart_grid is-header w-hidden-tiny">
                    <div id="w-node-_4ff72092-e0e6-d591-6af1-5b10f594a632-f594a625" class="cart_table-header">
                      <p class="text-size-regular">{{ section.settings.text_product }}</p>
                    </div>
                    <div class="cart_table-header">
                      <p class="text-size-regular">{{ section.settings.text_quantity }}</p>
                    </div>
                    <div id="w-node-_4ff72092-e0e6-d591-6af1-5b10f594a638-f594a625" class="cart_table-header">
                      <p class="text-size-regular">{{ section.settings.text_price }}</p>
                    </div>
                  </div>
                  {% for item in cart.items %}<div li-for="item in cart.items" class="cart_grid">
                    <div id="w-node-_4ff72092-e0e6-d591-6af1-5b10f594a63c-f594a625" class="display-inlineflex">
                      <a href="{{ item.url }}" li-object:href="item.url" id="w-node-_4ff72092-e0e6-d591-6af1-5b10f594a63d-f594a625" class="w-inline-block"><img src="{{ item.image | image_url: width: 200 }}" li-object:src="item.image | image_url: width: 200" alt="" class="cart_image"></a>
                      <div class="cart_info-order">
                        <a href="{{ item.url }}" li-object:href="item.url" li-object="item.title" class="text-size-regular">{{ item.title }}</a>
                        <div class="hide">
                          <p item="price" class="cart_price">$13</p>
                          <p item="original-price" class="cart_price away">$18</p>
                        </div>
                      </div>
                    </div>
                    <div id="w-node-_4ff72092-e0e6-d591-6af1-5b10f594a647-f594a625" class="cart_table-item anzahl">
                      <div class="cart_quantity-wrapper">
                        <div>Quantity</div><input class="cart_quantity w-input" li-attribute:value="{{ item.quantity }}" maxlength="256" name="updates[]" data-name="updates[]" placeholder="1" li-attribute:name="updates[]" type="number" id="updates[]" required="">
                      </div>
                      <a href="{{ item.url_to_remove }}" li-object:href="item.url_to_remove" class="text-size-tiny text-style-link text-align-center w-inline-block">
                        <div>{{ section.settings.text_remove }}</div>
                      </a>
                    </div>
                    <div id="w-node-_4ff72092-e0e6-d591-6af1-5b10f594a64f-f594a625" class="cart_price-info">
                      <div li-object="item.final_line_price | money" class="text-size-regular">{{ item.final_line_price | money }}</div>
                      {% if item.original_line_price &gt; item.final_line_price %}<div li-if="item.original_line_price &gt; item.final_line_price">
                        <div li-object="item.original_line_price | money" class="text-size-regular text-style-strikethrough">{{ item.original_line_price | money }}</div>
                      </div>{% endif %}
                    </div>
                  </div>{% endfor %}
                </div>
                <div id="w-node-_4ff72092-e0e6-d591-6af1-5b10f594a655-f594a625" class="cart_wrapper-right">
                  <div id="w-node-_4ff72092-e0e6-d591-6af1-5b10f594a656-f594a625" class="cart_table outline-solid-1">
                    <div class="cart_grid is-header w-hidden-tiny">
                      <div id="w-node-_4ff72092-e0e6-d591-6af1-5b10f594a658-f594a625" class="cart_table-header">
                        <p class="text-size-regular">{{ section.settings.text_summary }}</p>
                      </div>
                    </div>
                    <div class="cart_content">
                      <div class="cart_wrapper">
                        <div class="cart_details-wrapper">
                          <div class="text-size-regular">{{ section.settings.text_sum_inkl_vat }}</div>
                          <div class="cart_details-price">
                            {% if cart.total_discounts &gt; 0 %}<div li-if="cart.total_discounts &gt; 0">
                              <div li-object="cart.original_total_price | money" class="text-style-strikethrough">{{ cart.original_total_price | money }}</div>
                            </div>{% endif %}
                            <div li-object="cart.total_price | money" class="heading-style-h3">{{ cart.total_price | money }}</div>
                          </div>
                        </div>
                        {% if cart.total_discounts &gt; 0 %}<div li-if="cart.total_discounts &gt; 0" class="cart_details-wrapper">
                          <div class="text-size-regular">{{ section.settings.text_discount }}</div>
                          <div li-object="cart.total_discount | money">{{ cart.total_discount | money }}</div>
                        </div>{% endif %}
                      </div>
                      <div class="cart_action"><input type="submit" data-wait="Please wait..." li-attribute:value="Checkout" li-attribute:name="checkout" class="button w-button" value="Checkout"><input type="submit" data-wait="Please wait..." li-attribute:value="Update cart" class="button is-outline w-button" value="Update"></div>
                    </div>
                  </div>
                  <div class="cart_table outline-solid-1">
                    <div class="cart_grid is-header w-hidden-tiny">
                      <div id="w-node-_4ff72092-e0e6-d591-6af1-5b10f594a670-f594a625" class="cart_table-header">
                        <p class="text-size-regular">{{ section.settings.text_comment }}</p>
                      </div>
                    </div>
                    <div class="cart_content">
                      <div class="form_field-wrapper"><textarea class="form_input is-text-area w-input" maxlength="5000" name="note" data-name="field" placeholder="Comment" li-attribute:name="note" id="CartNote"></textarea></div>
                    </div>
                  </div>
                </div>
              </form>
              <div class="w-form-done">
                <div>Thank you! Your submission has been received!</div>
              </div>
              <div class="w-form-fail">
                <div>Oops! Something went wrong while submitting the form.</div>
              </div>
            </div>{% endif %}
            {% if cart.item_count == 0 %}<div li-if="cart.item_count == 0" class="cart_empty outline-solid-1">
              <div>{{ section.settings.text_cart_empty }}</div>
              <a href="{{ routes.root_url }}" li-object:href="routes.root_url" class="button w-button">Shop Now</a>
            </div>{% endif %}
          </div>
        </div>
