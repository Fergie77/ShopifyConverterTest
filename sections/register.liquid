<div class="login_component">
          <div id="w-node-_020413c5-8dec-2c46-93a1-274ba44a608a-a44a6088" class="login_left">
            <div id="w-node-_020413c5-8dec-2c46-93a1-274ba44a608b-a44a6088" class="login_tabs">
              <div class="login_form w-form">
                <h1 li-settings:text="Create Account" class="heading-style-h2 text-align-center">{{ section.settings.text_create_account }}</h1>
                <form method="get" li-form:return_to="/" name="email-form" data-name="Email Form" li-form="create_customer" id="email-form" class="form_form" data-wf-page-id="68e5827dc33ba2f819dc55ad" data-wf-element-id="020413c5-8dec-2c46-93a1-274ba44a608f">
                  {% if form.errors %}<div li-if="form.errors" class="form_error-message">
                    <div li-object="form.errors | default_errors" class="text-size-regular text-color-red text-align-center">{{ form.errors | default_errors }}</div>
                  </div>{% endif %}
                  <div class="form_field-wrapper"><label for="field-5" class="form_hidden-label">First&nbsp;Name</label><input class="form_input w-input" maxlength="256" name="field-5" data-name="Field 5" placeholder="First Name" li-attribute:name="customer[first_name]" type="text" id="field-5" required=""></div>
                  <div class="form_field-wrapper"><label for="field-5" class="form_hidden-label">Last Name</label><input class="form_input w-input" maxlength="256" name="field-5" data-name="Field 5" placeholder="Last Name" li-attribute:name="customer[last_name]" type="text" id="field-5" required=""></div>
                  <div class="form_field-wrapper"><label for="field-5" class="form_hidden-label">Email Address</label><input class="form_input w-input" maxlength="256" name="field-5" data-name="Field 5" placeholder="Email Address" li-attribute:name="customer[email]" type="email" id="field-5" required=""></div>
                  <div class="form_field-wrapper"><label for="field-5" class="form_hidden-label">Password</label><input class="form_input w-input" maxlength="256" name="field-5" data-name="Field 5" placeholder="Password" li-attribute:name="customer[password]" type="password" id="field-5" required=""></div><label class="w-checkbox form_checkbox"><input type="checkbox" name="" data-name="" required="" class="w-checkbox-input form_checkbox-icon"><span li-settings:text="Checkbox Legal" class="form_checkbox-label w-form-label">{{ section.settings.text_checkbox_legal }}</span></label><label class="w-checkbox form_checkbox"><input li-attribute:name="customer[accepts_marketing]" type="checkbox" name="" data-name="" class="w-checkbox-input form_checkbox-icon"><span li-settings:text="Checkbox Marketing" class="form_checkbox-label w-form-label">{{ section.settings.text_checkbox_marketing }}</span></label><input type="submit" data-wait="Please wait" id="w-node-_020413c5-8dec-2c46-93a1-274ba44a60ab-a44a6088" class="button max-width-full w-button" value="Register now">
                </form>
                <div class="text-size-regular text-align-center"><span li-settings:text="Already have an Account">{{ section.settings.text_already_have_an_account }}</span>
                  <a href="{{ routes.account_login_url }}" li-object:href="routes.account_login_url" class="text-style-link text-weight-bold">Login now</a>
                </div>
                <div class="hide w-form-done">
                  <div>Thank you! Your submission has been received!</div>
                </div>
                <div class="hide w-form-fail">
                  <div>Oops! Something went wrong while submitting the form.</div>
                </div>
              </div>
            </div>
          </div>
          <div id="w-node-_020413c5-8dec-2c46-93a1-274ba44a60b6-a44a6088" class="login_image-wrapper">{% if section.settings.image_bild %}
{{ section.settings.image_bild | image_url: width: 800 | image_tag: loading: 'lazy', class: 'login_image', id: 'w-node-_020413c5-8dec-2c46-93a1-274ba44a60b7-a44a6088' }}
{% else %}
<img src="{{ 'hero-apparel-1.svg' | asset_url }}" loading="lazy" id="w-node-_020413c5-8dec-2c46-93a1-274ba44a60b7-a44a6088" alt="" class="login_image">
{% endif %}</div>
        </div>
